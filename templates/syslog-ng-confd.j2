{% for s in item.sources %}{% for (source, drivers) in s.iteritems() %}
source {{source}} {
{% for d in drivers %}{% for driver, params in d.iteritems() %}
  {{driver}}( {% for p in params %}{% for (key, value) in p.iteritems() %}{{key}}({{value}}) {% endfor %}{% endfor %});
{% endfor %}{% endfor %}
};

{% endfor %}{% endfor %}

{% for f in item.filters %}{% for (filter, logic) in f.iteritems() %}
filter {{filter}} {
{% for line in logic %}
  {{line}}
{% endfor %}
};

{% endfor %}{% endfor %}

{% for p in item.parsers %}{% for (parser, logic) in p.iteritems() %}
parser {{parser}} {
{% for line in logic %}
  {{line}}
{% endfor %}
};

{% endfor %}{% endfor %}

{% for d in item.destinations %}{% for (dest, logic) in d.iteritems() %}
destination {{dest}} {
{% for line in logic %}
  {{line}}
{% endfor %}
};

{% endfor %}{% endfor %}

{% for l in item.logs %}
{% if l.not is defined %}#{% set log = l.not %}{% endif %}{% if l.log is defined %}{% set log = l.log %}{% endif %}
{% if log is defined %}
log { {% for p in log %}{% for (key, value) in p.iteritems() %}{{key}}({{value}}); {% endfor %}{% endfor %}};
{% endif %}
{% endfor %}
